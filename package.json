{
  "name": "wmediaproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint \"**/*.+(js|vue)\" --ignore-path .gitignore",
    "lint-fix": "eslint \"**/*.+(js|vue)\" --fix --ignore-path .gitignore",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|vue)\"",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|json|vue)\"",
    "ci": "vue-cli-service build --mode ci",
    "dev": "vue-cli-service serve --mode dev",
    "prod": "vue-cli-service build --mode prod"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.20",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
